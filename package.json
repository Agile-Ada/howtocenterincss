{
  "name": "howtocenterincss",
  "version": "1.0.0",
  "description": "",
  "main": "js/app.js",
  "scripts": {
    "watch": "npm run js-watch & npm run css-watch",
    "js-watch": "watchify -o build/bundle.js -v -d .",
    "js-minify": "NODE_ENV=production browserify . | uglifyjs -cm > build/bundle.min.js",
    "css-watch": "catw -c 'lessc -' 'bower_components/normalize.css/normalize.css' 'css/*.less' -o build/bundle.css -v"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "browserify": "^8.1.1",
    "envify": "^3.2.0",
    "reactify": "^1.0.0",
    "uglify-js": "^2.4.16",
    "watchify": "^2.2.1",
    "catw": "^0.2.0",
    "less": "^2.3.1"
  },
  "browserify": {
    "transform": [
      [
        "reactify",
        {
          "stripTypes": true,
          "es6": true,
          "harmony": true
        }
      ],
      "envify"
    ]
  },
  "dependencies": {
    "fastclick": "^1.0.3",
    "keymirror": "^0.1.1",
    "react": "^0.12.2",
    "react-addons": "^0.9.0",
    "shortid": "^2.1.3"
  }
}
